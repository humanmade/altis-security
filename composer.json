{
	"name": "altis/security",
	"description": "Security module for Altis",
	"type": "library",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Human Made",
			"email": "hello@humanmade.com"
		}
	],
	"autoload": {
		"files": [
			"inc/namespace.php",
			"inc/limit_login_attempts/namespace.php",
			"inc/passwords/namespace.php",
			"inc/php_basic_auth/namespace.php"
		],
		"classmap": [
			"inc/",
			"inc/stream/"
		]
	},
	"require": {
		"php": ">=7.1",
		"bjeavons/zxcvbn-php": "^1.1.0",
		"altis/browser-security": "^1.1.1",
		"humanmade/hm-limit-login-attempts": "1.1.0",
		"humanmade/php-basic-auth": "^1.1.0",
		"humanmade/require-login": "~1.0.2",
		"humanmade/stream": "3.4.2",
		"humanmade/two-factor": "0.2"
	},
	"extra": {
		"altis": {
			"install-overrides": [
				"altis/browser-security",
				"humanmade/hm-limit-login-attempts",
				"humanmade/php-basic-auth",
				"humanmade/require-login",
				"humanmade/stream",
				"humanmade/two-factor"
			]
		}
	}
}
